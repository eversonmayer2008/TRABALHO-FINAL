import sqlite3

conn = sqlite3.connect(':memory:')
cursor = conn.cursor()

cursor.execute('''
CREATE TABLE alunos_tb (
    id TEXT PRIMARY KEY,
    nome TEXT,
    turma TEXT
)
''')

# Dados de exemplo
cursor.execute("INSERT INTO alunos_tb VALUES ('a1', 'Maria', '3A')")
cursor.execute("INSERT INTO alunos_tb VALUES ('a2', 'Pedro', '2B')")

print("Alunos:")
for row in cursor.execute("SELECT * FROM alunos_tb"):
    print(row)

conn.close()
